VehicleDisplayContent {
  top: 0;
  left: 0;
  height: 128;
  width: 128;
  min-height: 128;
  max-height: 128;
  min-width: 128;
  max-width: 128;

  Div {
    height: 128;
    width: 128;

    .warning {
      width: 14;
      height: 14;
      top: 83%;
      alignment-x: center;
      visible: false;
      z-index: 2;
    }

    .speedLimiter {
      width: 12;
      height: 12;
      top: 78%;
      right: 65%;
      visible: false;
      z-index: 2;
    }

    .info {
      top: 32%;
      alignment-x: center;
      max-lines: 1;
      overflow-strategy: clip;
      font-size: small;
      z-index: 2;
    }

    .speed {
      top: 40%;
      alignment-x: center;
      max-lines: 1;
      overflow-strategy: clip;
      font-size: 2.5;
      z-index: 2;
    }

    .background {
      width: 128;
      height: 128;
      right: 0;
      top: 0;
      z-index: 1;
    }
  }
}